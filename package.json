{
    "name": "clippy",
    "version": "1.0.2",
    "description": "Member clippy?",
    "main": "./dist/main.bundle.js",
    "scripts": {
        "clean": "node clean.js",
        "build": "npm run clean && webpack --config ./webpack.prod.js",
