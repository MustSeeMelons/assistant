        "classnames": "2.2.6",
        "winston": "3.2.1"
    },
    "build": {
        "appId": "fake_clippy",
        "productName": "Fake Clippy",
        "files": [
            "./dist/**/*"
        ],
        "win": {
            "target": "portable",
            "icon": "./src/resources/images/clippy.png"
        }
    }
