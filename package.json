    "repository": {
        "type": "git"
    },
    "keywords": [
        "React",
        "Typescript",
        "Electron"
    ],
    "author": "Strautins",
    "devDependencies": {
        "@types/react": "^16.8.19",
        "@types/react-dom": "^16.8.4",
        "@types/lodash": "4.14.134",
        "@types/html-entities": "1.2.16",
        "@types/classnames": "2.2.8",
        "@types/winston": "2.4.4",
        "css-loader": "^2.1.1",
        "electron": "^18.3.7",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.12.0",
        "prettier": "^1.17.1",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "ts-loader": "^6.0.1",
        "typescript": "^3.5.1",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.5",
        "webpack-dev-server": "^3.4.1",
        "electron-builder": "20.44.4",
        "copy-webpack-plugin": "5.0.3",
        "del": "5.1.0"
    },
    "dependencies": {
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
